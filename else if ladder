//program to check given character is alphabet,digit,or special symbol.(file name)
#include<stdio.h>
int main(){
   char ch;
   printf("Enter the number :");
   scanf("%c",&ch);
   if((ch>=65 && ch<=90) || (ch>=97 && ch<=122))
   printf("%c is Alphabet\n",ch);
   else if(ch>=48 && ch<=57)
   printf("%cis digit\n",ch);
   else 
   printf("%c is special character \n",ch);
   return 0;

}
