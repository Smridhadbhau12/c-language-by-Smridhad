//Program to find the largest among two numbers.
//if else statement.
#include<stdio.h>
int main(){
    int n1,n2;
    printf("Enter n1 =");
    scanf("%d",&n1);
    printf("Enter n2 =");
    scanf("%d",&n2);

    if(n1>n2)
    {
        printf("%d n1>n2\n",n1);
    }
    else
    {
        printf("%d n1<n2\n",n2);

    }
    
    return 0;

}
