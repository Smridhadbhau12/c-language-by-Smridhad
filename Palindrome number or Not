#include<stdio.h>
int main(){   
  int n,c,rem,sum=0;
  printf("Enter any number ");
  scanf("%d",&n);
   c=n;  // storing value of n in c variable;
   while(n>0){
      rem=n%10;
      sum = rem+(sum*10);
      n=n/10;
   }
   if(c==sum){
      printf("Palindrome Number");
   }else{
      printf("Not ");
   }
   return 0;
}
